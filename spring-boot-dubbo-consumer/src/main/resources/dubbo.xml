<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application  name="dubbo-provider" owner="dubbo-provider"/>
    <dubbo:registry protocol="zookeeper" address="my.self.com:2181" client="zkclient"/>
    <dubbo:protocol name="dubbo" port="-1"/>
    <!--<dubbo: lazy="true" check="false" timeout="30000"/>-->



    <dubbo:reference interface="com.company.project.remote.UserDubboService" id="userDubboService" timeout="10000" lazy="true"/>
</beans>